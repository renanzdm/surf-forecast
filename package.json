{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Complete API built with Node.js and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
    "tests": "yarn run lint && yarn run style:check && yarn run test:unit && yarn run test:functional",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "engines": {
    "node": "v14.15.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.4",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.1",
    "express-pino-logger": "^5.0.0",
    "express-rate-limit": "^5.2.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.11.8",
    "node-cache": "^5.1.2",
    "pino": "^6.3.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.3",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.13.2",
    "@types/node-cache": "^4.2.5",
    "@types/pino": "^6.3.0",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "jest": "^26.0.1",
    "nock": "^12.0.3",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^3.8.3"
  }
}
